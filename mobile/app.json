{
  "expo": {
    "name": "DevRadar",
    "description": "Dev Radar App",
    "slug": "DevRadar",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }, 
    "android" : {
      "package": "com.mobile.devRadar",
      "config": {
        "googleMaps": {
          "apiKey": "<YOUR_API_KEY>"
        }
      }
    }
  }
}
